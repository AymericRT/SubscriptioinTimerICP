{
  "name": "ic-alloy-toolkit",
  "image": "ghcr.io/dfinity/icp-dev-env:13",
  "forwardPorts": [
    4943
  ],
  "portsAttributes": {
    "4943": {
      "label": "dfx",
      "onAutoForward": "ignore"
    }
  },
  "workspaceMount": "source=${localWorkspaceFolder},target=/ic,type=bind",
  "workspaceFolder": "/ic",
  "customizations": {
    "vscode": {
      "extensions": [
        "tamasfe.even-better-toml",
        "rust-lang.rust-analyzer"
      ]
    },
    "codespaces": {
      "openFiles": [
        "README.md"
      ]
    }
  }
}
